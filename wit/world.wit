package scala-wasm:component-example;

interface name {
    name: func() -> string;
}

interface greet {
    greet: func();
}

world hello-world {
    /// currently, scala-wasm can't import inline functions :(
    import name;
    export greet;

    /// required for println
    import wasi:cli/stdout@0.2.0;
    import wasi:io/streams@0.2.0;

}

